generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Hose {
  id                 BigInt        @id @default(autoincrement())
  model              String
  hose_size          String
  DN_diameter        Int
  initial_price      Int
  market_price       Int
  working_pressure   Int
}

model Fittings {
  id                 BigInt        @id @default(autoincrement())
  model              String
}